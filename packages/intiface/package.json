{
  "name": "intiface-desktop",
  "description": "Desktop Intiface Application",
  "author": "Nonpolynomial Labs, LLC",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "tslint": "tslint --project tsconfig.json --outputAbsolutePaths -c ../../tslint.json \"src/**/*.ts\" \"tests/**/*.ts\"",
    "tslint:fix": "tslint --project tsconfig.json --outputAbsolutePaths -c ../../tslint.json --fix semicolon,comma,no-var-keyword,ordered-imports \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "intiface-core-library": "*",
    "intiface-protocols": "*",
    "vue": "^2.6.8",
    "vue-class-component": "^7.0.1",
    "vue-property-decorator": "^7.3.0",
    "vue-router": "^3.0.2",
    "vuetify": "^1.5.4",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@vue/cli-plugin-typescript": "^3.4.1",
    "@vue/cli-plugin-unit-jest": "^3.4.1",
    "@vue/cli-service": "^3.4.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "electron": "^4.0.6",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333",
    "vue-cli-plugin-electron-builder": "^1.1.0",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.8",
    "vuetify-loader": "^1.2.1"
  },
  "main": "background.js"
}
